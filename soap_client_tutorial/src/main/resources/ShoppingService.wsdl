					</xs:documentation>
					<xs:appinfo>
						<CallInfo>
							<CallName>GetUserProfile</CallName>
							<Details>IncludeSelector: FeedbackDetails</Details>
							<Returned>Conditionally</Returned>
						</CallInfo>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="ItemPrice" type="ns:AmountType" minOccurs="0">
