								<Returned>Conditionally</Returned>
							</CallInfo>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="LogisticPlanType" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						local logistic plan type for this service
					</xs:documentation>
					<xs:appinfo>
						<CallInfo>
							<CallName>GetShippingCosts</CallName>
							<Returned>Conditionally</Returned>
						</CallInfo>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
