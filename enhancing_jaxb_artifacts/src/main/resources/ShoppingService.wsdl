					<xs:appinfo>
						<CallInfo>
							<CallName>GetUserProfile</CallName>
							<Details>IncludeSelector: FeedbackHistory</Details>
							<Returned>Always</Returned>
						</CallInfo>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
